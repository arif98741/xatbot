{
    "name": "xatbot/bot",
    "description": "xatbot Bot",
    "require": {
        "php": "^8.1",
        "illuminate/database": "^5.4",
        "monolog/monolog": "^1.22",
        "abraham/twitteroauth": "^7.0.0",
        "jwilsson/spotify-web-api-php": "^6.0.1",
        "ext-sockets": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.0",
        "phpunit/phpunit": ">=10.5.18"
    },
    "autoload":{
        "psr-4": {
            "Xatbot\\Bot\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        }
    }
}
